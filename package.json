{
  "name": "@onsite/node-fetch",
  "version": "3.3.0-rc.1",
  "description": "A light-weight module that brings Fetch API to node.js",
  "main": "./src/index.js",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "files": [
    "src",
    "@types/index.d.ts"
  ],
  "types": "./@types/index.d.ts",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "brVars": {
    "name": "NodeFetch",
    "package": "node.fetch"
  },
  "scripts": {
    "clean": "rimraf lib/* dist/* compiled/* && mkdirp lib dist compiled",
    "compile": "tsc -p ./tsconfig.json",
    "rollup-cjs": "rollup -c config/rollup.config.cjs.js && rollup -c config/rollup.config.browser.cjs.js",
    "rollup-es": "rollup -c config/rollup.config.es.js && rollup -c config/rollup.config.browser.es.js",
    "rollup": "npm-run-all --parallel rollup-cjs rollup-es",
    "browserify:rollup": "browserify -t [ babelify --global --presets [ @babel/preset-env ] ] -t brfs -p bundle-collapser/plugin -s $npm_package_brVars_name lib/index.browser.js > dist/$npm_package_brVars_package.js",
    "browserify": "npm run browserify:rollup",
    "minify": "uglifyjs -mc < dist/$npm_package_brVars_package.js > dist/$npm_package_brVars_package.min.js",
    "build:esbuild": "esbuild src/*.ts --format=esm --outdir=esm && esbuild src/*.ts --format=cjs --outdir=cjs",
    "build:prod": "npm-run-all clean rollup browserify minify",
    "build:dev": "npm-run-all clean rollup browserify",
    "build": "npm run build:prod",
    "test": "mocha",
    "coverage": "c8 report --reporter=text-lcov | coveralls",
    "test-types": "tsd",
    "lint": "xo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chorpler/node-fetch.git"
  },
  "keywords": [
    "fetch",
    "http",
    "promise",
    "request",
    "curl",
    "wget",
    "xhr",
    "whatwg"
  ],
  "author": "David Sargeant",
  "contributors": [
    "David Frank"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chorpler/node-fetch/issues"
  },
  "homepage": "https://github.com/chorpler/node-fetch",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/node-fetch"
  },
  "dependencies": {
    "data-uri-to-buffer": "^4.0.0",
    "fetch-blob": "^3.1.4",
    "formdata-polyfill": "^4.0.10"
  },
  "devDependencies": {
    "abort-controller": "^3.0.0",
    "abortcontroller-polyfill": "^1.7.1",
    "babelify": "^10.0.0",
    "brfs": "^2.0.1",
    "browserify": "^16.2.3",
    "busboy": "^1.4.0",
    "c8": "^7.7.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-iterator": "^3.0.2",
    "chai-string": "^1.5.0",
    "coveralls": "^3.1.0",
    "cross-env": "^5.2.0",
    "esbuild": "^0.14.21",
    "eslint": "^8.21.0",
    "form-data": "^4.0.0",
    "formdata-node": "^4.2.4",
    "mocha": "^9.1.3",
    "p-timeout": "^5.0.0",
    "stream-consumers": "^1.0.1",
    "rollup": "^1.2.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-inject": "^2.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "standard-version": "^5.0.0",
    "ts-node": "^10.7.0",
    "tsd": "^0.14.0",
    "tslib": "^1.9.3",
    "typescript": "^4.5.5",
    "uglify-js": "^3.4.9",
    "xo": "^0.39.1"
  },
  "tsd": {
    "cwd": "@types",
    "compilerOptions": {
      "esModuleInterop": true
    }
  },
  "xo": {
    "envs": [
      "node",
      "browser"
    ],
    "ignores": [
      "example.js"
    ],
    "rules": {
      "complexity": 0,
      "import/extensions": 0,
      "import/no-useless-path-segments": 0,
      "import/no-anonymous-default-export": 0,
      "import/no-named-as-default": 0,
      "unicorn/import-index": 0,
      "unicorn/no-array-reduce": 0,
      "unicorn/prefer-node-protocol": 0,
      "unicorn/numeric-separators-style": 0,
      "unicorn/explicit-length-check": 0,
      "capitalized-comments": 0,
      "node/no-unsupported-features/es-syntax": 0,
      "@typescript-eslint/member-ordering": 0
    },
    "overrides": [
      {
        "files": "test/**/*.js",
        "envs": [
          "node",
          "mocha"
        ],
        "rules": {
          "max-nested-callbacks": 0,
          "no-unused-expressions": 0,
          "no-warning-comments": 0,
          "new-cap": 0,
          "guard-for-in": 0,
          "unicorn/no-array-for-each": 0,
          "unicorn/prevent-abbreviations": 0,
          "promise/prefer-await-to-then": 0,
          "ava/no-import-test-files": 0
        }
      }
    ]
  },
  "runkitExampleFilename": "example.js",
  "release": {
    "branches": [
      "+([0-9]).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  }
}
